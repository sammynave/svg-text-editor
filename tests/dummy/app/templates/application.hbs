{{!--#svg-editor src="/dummy.svg" targetIds=targetIds as |e|}}
  {{#if e.loading}}
    wait
  {{else}}
    {{e.svg}}
    {{text-tracker
      styles=e.imposterStyles
      content=e.content
      update=(action e.update)}}
  {{/if}}
{{/svg-editor --}}

